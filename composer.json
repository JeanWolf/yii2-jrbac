{
  "name": "jext/yii2-jrbac",
  "description": "JRBAC - Yii2 RBAC admin control panel and dashboard that's based on Bootstrap 3",
  "homepage": "http://blog.jeen.wang/",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "admin",
    "rbac",
    "extension",
    "module"
  ],
  "authors": [
    {
      "name": "Jeen Wang",
      "email": "jeen@vsfor.com"
    }
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/JeanWolf/yii2-jrbac/issues"
  },
  "require": {
      "yiisoft/yii2": "2.*",
      "yiisoft/yii2-bootstrap": "2.*"
  },
  "autoload": {
      "psr-4": {
          "jext\\": ""
      }
  },
  "extra": {
      "branch-alias": {
          "dev-master": "1.0-dev"
      }
  }
}

